<h1>Search Tasks</h1>
{{#if error}}<span>{{error}}</span>{{/if}}
<form class="form-inline" action="/tasks/search" method="post">
  <div class="form-group">
    <input type="text" name="id" class="form-control" placeholder="Search Tasks">
  </div>
  <input type="submit" class="btn btn-primary" value="search"/>
</form>
